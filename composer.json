{
    "name": "kylearch/ephemeral-users",
    "description": "Laravel package for ephemeral (non-persistent) user instances",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Arch",
            "email": "kaarch@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KyleArch\\EphemeralUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KyleArch\\EphemeralUsers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KyleArch\\EphemeralUsers\\EphemeralUserServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
